{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@": ["."],
      "@/*": ["./*"],
      "@components": ["components/index"],
      "@components/*": ["components/*"],
      "@utils": ["utils"],
      "@utils/*": ["utils/*"],
      "@const": ["const/index"],
      "@const/*": ["const/*"],
      "@lang": ["lang/index"],
      "@net": ["utils/net/index"],
      "@algorithm": ["utils/algorithms/index"],
      "@ms-graph": ["utils/ms-graph/index"],
      "@ms-graph/*": ["utils/ms-graph/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "components/music.jsx"],
  "exclude": ["node_modules"]
}
